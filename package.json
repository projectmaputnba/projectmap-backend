        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.3.8",
        "@nestjs/swagger": "^7.3.1",
        "bcrypt": "^5.1.1",
        "dotenv": "^16.4.5",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.4.0",
        "nest": "^0.1.6",
        "nodemailer": "^6.9.15",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "reflect-metadata": "^0.2.2",
        "rimraf": "^5.0.7",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.3.0",
        "@nestjs/cli": "^10.3.2",
        "@nestjs/schematics": "^10.1.1",
        "@nestjs/testing": "^10.3.10",
        "@swc/cli": "^0.3.14",
